        "webpack-dev-server": "^3.1.9",
        "webpack-merge": "^4.1.4"
    },
    "engines": {
        "node": ">=8.11.4",
        "npm": "6.4.1"
    },
    "browserslist": [
        "last 3 Chrome versions",
        "last 3 Firefox versions",
        "Safari >= 10",
        "Explorer >= 11",
        "Edge >= 12",
        "iOS >= 10",
        "Android >= 6"
    ]
}
