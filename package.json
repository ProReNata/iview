{
  "name": "iview",
  "version": "3.0.0-alpha.10",
  "title": "iView",
  "description": "A high quality UI components Library with Vue.js",
  "homepage": "http://www.iviewui.com",
  "keywords": [
    "iview",
    "vue",
    "vue.js",
    "component",
    "components",
    "ui",
    "framework"
  ],
  "main": "dist/iview.min.js",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --content-base test/ --open --inline --hot --compress --history-api-fallback --port 8081",
    "lint": "eslint -f 'node_modules/eslint-friendly-formatter' --ext .js,.vue,.json .",
    "lint-fix": "npm run lint -- --fix",
    "lint:css": "stylelint '**/*.{css,less,vue}'",
    "lint-fix:css": "npm run lint:css -- --fix",
    "lint:all": "npm run lint:css & npm run lint",
    "lint-fix:all": "npm run lint-fix:css & npm run lint-fix",
    "security": "npm audit",
    "security-fix": "npm run security -- fix",
    "unit": "cross-env NODE_ENV=production karma start test/unit/karma.conf.js --single-run",
    "test": "npm run unit",
    "build": "npm run clean-dist && cross-env NODE_ENV=production webpack && npm run css:minify",
    "build-report": "npm run build -- --env.report",
    "dev-reset": "npm run prepare-env && npm run clean-modules && npm install && npm run dev",
    "storybook": "start-storybook -p 9001 -c Storybook/.storybook",
    "clean-dist": "rimraf dist",
    "clean-modules": "rimraf node_modules",
    "prepare-env": "bash DevTools/Build/prepare_env.sh",
    "prettier-check": "eslint --print-config . | eslint-config-prettier-check",
    "css:minify": "postcss -m --config cssnano dist/iview.css -o dist/iview.min.css"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ProReNata/iview"
